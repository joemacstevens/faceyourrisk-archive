/* Clock Animation Classes */
:root {
  --clock-frames: 37;
}

.clock {
  position: relative;
  width: 100%;
  height: 100%;
  background-size: cover;
  background-position: center;
  background-repeat: no-repeat;
}

@for $i from 1 through var(--clock-frames) {
  .clock_#{$i} .clock_#{$i} {
    display: block;
  }
  
  .clock_#{$i} .clock_#{$i} {
    display: block;
  }
}

